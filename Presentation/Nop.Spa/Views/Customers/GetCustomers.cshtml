@model IEnumerable<Nop.Spa.DTOs.CustomerApi>

@{
    ViewData["Title"] = "GetCustomers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<ul>
    @foreach (var customer in Model)
    {
        <li>@customer.FirstName @customer.LastName - <a href="/UpdateCustomer/@customer.Id">Reset Last Name</a></li>
    }
</ul>